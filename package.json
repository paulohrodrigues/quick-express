{
    "name": "quick-express",
    "version": "0.5.0",
    "description": "A ready to use rest API structure made with Express.js and Typescript.",
    "repository": "github:scriptaria/quick-express",
    "main": "build/core/bootstrap.js",
    "scripts": {
        "build": "cleandir build && tsc",
        "serve": "node build/core/bootstrap.js start prod",
        "database": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "start": "npm run build && npm run serve",
        "test": "npm run build && mocha \"build/modules/**/spec.js\"",
        "doc": "npm run build && apidoc -i build/ -o doc/",
        "quick": "ts-node src/core/cli.ts"
    },
    "author": {
        "name": "Jos√© Camelo de Freitas",
        "email": "cmpjosf@outlook.com",
        "url": "https://scriptaria.com/"
    },
    "license": "MIT",
    "apidoc": {
        "title": "Quick Express - Documentation"
    },
    "_moduleAliases": {
        "src": "build"
    },
    "dependencies": {
        "apidoc": "0.19.0",
        "bcrypt": "3.0.6",
        "body-parser": "1.19.0",
        "boolean": "3.0.0",
        "camelcase": "5.3.1",
        "clean-dir": "1.0.3",
        "cors": "2.8.5",
        "cross-env": "6.0.3",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "jsonwebtoken": "8.5.1",
        "make-runnable": "1.3.6",
        "module-alias": "2.2.2",
        "mysql": "2.17.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.3",
        "slug": "1.1.0",
        "tsconfig-paths": "3.9.0",
        "tslint": "5.20.1",
        "tslint-eslint-rules": "5.4.0",
        "typeorm": "0.2.21",
        "typescript": "3.7.3"
    },
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/cors": "2.8.6",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.2",
        "@types/jsonwebtoken": "8.3.5",
        "@types/mocha": "5.2.7",
        "@types/node": "12.12.17",
        "@types/slug": "0.9.1",
        "@types/supertest": "2.0.8",
        "mocha": "5.2.0",
        "supertest": "4.0.2",
        "ts-node": "8.5.4"
    }
}